{
  "name": "beaf-station",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,json,md,scss,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "ant-design-vue": "^1.3.17",
    "axios": "0.21.1",
    "beaf-scripts": "^0.0.36",
    "element-ui": "2.5.4",
    "husky": "1.3.1",
    "json-server": "0.14.2",
    "lint-staged": "8.1.3",
    "lodash": "4.17.11",
    "prettier": "1.16.4",
    "vue": "^2.6.4",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.0"
  },
  "scripts": {
    "start": "PORT=3001 create-beaf-app start",
    "build": "create-beaf-app build",
    "server:mock": "json-server --watch mock/index.js --port 8081"
  },
  "proxy": {
    "/*": {
      "target": "localhost:3001",
      "secure": true,
      "changeOrigin": true,
      "pathRewrite": {
        "^/example": ""
      }
    }
  },
  "devDependencies": {
    "node-sass": "^4.11.0"
  },
  "peerDependencies": {}
}
